{
  "name": "prueba",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "node ./bin/www",
    "routes": "node scripts/routes.js",
    "db:prepare": "node scripts/prepare-db.js",
    "migrate": "npm run db:prepare && knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "npm run db:prepare && knex migrate:rollback --knexfile knexfile.js",
    "seed": "npm run db:prepare && knex seed:run --knexfile knexfile.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.6",
    "debug": "~2.6.9",
    "dotenv": "^17.3.1",
    "express": "~4.16.1",
    "express-list-endpoints": "^7.1.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  }
}
